{
  "name": "star-citizen",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/app",
    "build": "tsc -p tsconfig.json",
    "test:cov": "jest --config ./jest-e2e.json"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "redis": "^3.0.2",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.15",
    "@types/redis": "^2.8.28",
    "@types/ws": "^7.2.8",
    "jest": "^25.1.0",
    "supertest": "^6.0.0",
    "ts-jest": "25.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "json-summary"
    ],
    "testEnvironment": "node"
  }
}
